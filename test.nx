/*
  Prints an array of integers !
*/
void printGrid(int[] array, int nbColumns)
{
	let i = 1;
	for (case in array)
	{
		$print(case);
		if (i % nbColumns == 0)
		{
			$println("");
			i = 0;
		}
		
		i = i+1;
	}
}

